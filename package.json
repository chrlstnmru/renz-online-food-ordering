{
  "name": "renz-online-food-ordering",
  "version": "0.0.1",
  "devDependencies": {
    "@iconify/svelte": "^3.1.4",
    "@melt-ui/pp": "^0.1.4",
    "@melt-ui/svelte": "^0.62.1",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.27.4",
    "@tailwindcss/forms": "^0.5.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/throttle-debounce": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.14",
    "drizzle-kit": "^0.20.4",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.30.0",
    "postcss": "^8.4.24",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "svelte": "^4.0.5",
    "svelte-autosize": "^1.1.0",
    "svelte-check": "^3.6.0",
    "svelte-easy-crop": "^2.0.1",
    "svelte-file-dropzone": "^2.0.2",
    "svelte-sequential-preprocessor": "^2.0.1",
    "sveltekit-flash-message": "^2.2.2",
    "sveltekit-superforms": "^1.10.2",
    "tailwind-scrollbar": "^3.0.5",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2"
  },
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "db:gen": "bun drizzle-kit generate:pg",
    "db:pull": "bun drizzle-kit introspect:pg",
    "db:push": "bun drizzle-kit push:pg",
    "db:gen:admin": "bun drizzle-kit generate:pg --config=drizzle.admin.config.ts",
    "db:pull:admin": "bun drizzle-kit introspect:pg --config=drizzle.admin.config.ts",
    "db:push:admin": "bun drizzle-kit push:pg --config=drizzle.admin.config.ts",
    "db:gen:user": "bun drizzle-kit generate:pg --config=drizzle.user.config.ts",
    "db:pull:user": "bun drizzle-kit introspect:pg --config=drizzle.user.config.ts",
    "db:push:user": "bun drizzle-kit push:pg --config=drizzle.user.config.ts"
  },
  "type": "module",
  "dependencies": {
    "@lucia-auth/adapter-postgresql": "^2.0.2",
    "@lucia-auth/oauth": "^3.5.0",
    "@supabase/supabase-js": "^2.38.5",
    "drizzle-orm": "^0.29.0",
    "jsonwebtoken": "^9.0.2",
    "lucia": "^2.7.4",
    "moment": "^2.29.4",
    "nanoid": "^5.0.3",
    "node-gyp": "^10.0.1",
    "pg": "^8.11.3",
    "postgres": "^3.4.3",
    "resend": "^2.0.0",
    "svelte-select": "^5.8.1",
    "tailwind-variants": "^0.1.18",
    "throttle-debounce": "^5.0.0",
    "zod": "^3.22.4",
    "zod-form-data": "^2.0.2"
  }
}
